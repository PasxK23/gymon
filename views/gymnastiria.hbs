<div class="hero bg-gym">
  <h1 style="margin-top: 25vw; color:white">ΓΥΜΝΑΣΤΗΡΙΑ</h1>
</div>
<div class="container">
  <div class="obj-area">
    {{#each gyms}}
      <a href="/gyms/{{this.Location}}" class="card">
        <h3>{{this.Location}}</h3>
        <div class="divider"></div>
        <img
          src="/images/{{this.Location}}card.webp"
          alt="{{this.Location}}"
          width="400"
          height="300"
        />
      </a>
    {{/each}}
  </div>
</div>